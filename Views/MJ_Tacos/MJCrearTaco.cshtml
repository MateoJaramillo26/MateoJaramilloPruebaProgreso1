@model IEnumerable<MJPruebaProgreso1.Models.MJ_Tacos>

@{
    ViewData["Title"] = "Crea tu taco MJ";
}

<h1>Crea tu taco por Mateo Jaramillo</h1>

<p>
    <a asp-action="MJCreate">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MJ_NombreTaco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MJ_DescripcionDelTaco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MJ_PrecioTaco)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MJ_Fecha)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MJ_Guacamole)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MJ_NombreTaco)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MJ_DescripcionDelTaco)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MJ_PrecioTaco)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MJ_Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MJ_Guacamole)
            </td>
            <td>
                <a asp-action="MJEdit" asp-route-id="@item.TacoID">Edit</a> |
                <a asp-action="MJDetails" asp-route-id="@item.TacoID">Details</a> |
                <a asp-action="MJDelete" asp-route-id="@item.TacoID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
